{
    "manifest_version": 3,
    "name": "Hello World Extension",
    "version": "1.0",
    "description": "Shows a Hello World dialog on page load",
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_idle"
      }
    ],
    "permissions": []
  }